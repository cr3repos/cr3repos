<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="apple-touch-icon" href="../assets/img/CR-3-logo.png">
  <link rel="icon" type="image/png" href="../assets/img/CR-3-logo.png">
  <title>
    CR3 | Admin
  </title>
  <!--     Fonts and icons     -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/0.2.0/Chart.min.js" type="text/javascript"></script>
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
  <!-- Nucleo Icons -->
  <link href="../assets/css/nucleo-icons.css" rel="stylesheet" />
  <link href="../assets/css/nucleo-svg.css" rel="stylesheet" />
  <!-- Font Awesome Icons -->
  <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
  <link href="../assets/css/nucleo-svg.css" rel="stylesheet" />
  <!-- CSS Files -->
  <link id="pagestyle" href="../assets/css/argon-dashboard.css?v=2.0.2" rel="stylesheet" />
  <link rel="stylesheet" href="../assets/css/style.css">
</head>

<body class="g-sidenav-show bg-gray-200">
  <div class="min-height-300 bg-primary position-absolute w-100"></div>
  <aside class="sidenav navbar navbar-vertical navbar-expand-xs border-0 border-radius-xl my-3 fixed-start ms-4 ps bg-white " id="sidenav-main">
    <div class="sidenav-header">
      <i class="fas fa-times p-3 cursor-pointer text-secondary opacity-5 position-absolute end-0 top-0 d-none d-xl-none" aria-hidden="true" id="iconSidenav"></i>
      <a class="navbar-brand m-0" href="/dashboard">
        <img src="../assets/img/CR-3-logo-main.png" class="navbar-brand-img h-100" alt="main_logo">
        <span class="ms-1 font-weight-bold">CR3</span>
      </a>
    </div>
    <hr class="horizontal dark mt-0">
    <div class="collapse navbar-collapse  w-auto " id="sidenav-collapse-main">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link" href="/dashboard">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class="ni ni-tv-2 text-primary text-sm opacity-10"></i>
            </div>
            <span class="nav-link-text ms-1">Dashboard</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link " href="/profile">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class=" fa fa-solid text-sm opacity-10 text-warning fa-user"></i>
            </div>
            <span class="nav-link-text ms-1">Profile</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/downlines">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class="fa text-success text-sm opacity-10 fa-solid fa-list"></i>
            </div>
            <span class="nav-link-text ms-1">Downlines</span>
          </a>
        </li>
        <li class="nav-item mt-3">
          <h6 class="ps-4 ms-2 text-uppercase text-xs font-weight-bolder opacity-6">Payments</h6>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/payment-portal">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class=" fa fa-solid text-warning text-sm opacity-10 fa-coins"></i>
            </div>
            <span class="nav-link-text ms-1">ID activation</span>
          </a>
        </li>

        <li class="nav-item ">
          <a class="nav-link" href="/withdraw">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class=" fa fa-solid text-danger text-sm opacity-10 fa-wallet"></i>
            </div>
            <span class="nav-link-text ms-1">Withdraw</span>
          </a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/history">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class="fa fa-solid fa-clock text-primary text-sm opacity-10"></i>
            </div>
            <span class="nav-link-text ms-1">History</span>
          </a>
        </li>

        <li class="nav-item">
          <a class="nav-link " href="/transactions">
            <div class="icon icon-shape icon-sm border-radius-md text-center me-2 d-flex align-items-center justify-content-center">
              <i class="fa fa-solid fa-repeat text-info text-sm opacity-10"></i>
            </div>
            <span class="nav-link-text ms-1">Transactions</span>
          </a>
        </li>
      </ul>
    </div>
    <div class="sidenav-footer text-center mx-3 ">

      <a class="btn btn-danger btn-sm mb-0 w-70" href="/logout" type="button">Log out</a>
    </div>
  </aside>
  <main class="main-content border-radius-lg ">

    <!-- Navbar -->
    <nav class="navbar position-relative navbar-main navbar-expand-lg px-0 mx-4 shadow-none border-radius-xl " id="navbarBlur" data-scroll="false">
      <div class="container-fluid pt-3 py-1 px-3">
        <nav aria-label="breadcrumb">
          <a href="/naanthaDa">
            <h6 class="font-weight-bolder text-white mb-0">
              <img src="../assets/img/CR-3-logo-main.png" class="dashboard-logo float-left" alt="">
              Admin
            </h6>
          </a>
        </nav>
        <div class="collapse position-absolute end-0 px-3 navbar-collapse mt-sm-0 mt-2 me-md-0 me-sm-4" id="navbar">
          <ul class="navbar-nav  justify-content-end">

            <li class="nav-item dropdown px-3 pe-2 d-flex align-items-center">
              <a href="javascript:;" class="nav-link text-white p-0" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                <i class="fa fa-bell text-danger cursor-pointer"></i>
              </a>
            </li>
            <li class="nav-item px-3 d-xl-none ps-3 d-flex align-items-center">
              <a href="javascript:;" class="nav-link text-white p-0" id="iconNavbarSidenav">
                <div class="sidenav-toggler-inner">
                  <i class="sidenav-toggler-line bg-white"></i>
                  <i class="sidenav-toggler-line bg-white"></i>
                  <i class="sidenav-toggler-line bg-white"></i>
                </div>
              </a>
            </li>
            <li class="nav-item px-3 d-flex align-items-center">
              <a href="/profile" class="nav-link text-white p-0">
                <i class="fa fa-user fixed-plugin-button-nav cursor-pointer"></i>
              </a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    <!-- End Navbar -->
    <div class="container-fluid  pt-3">
      <div class="row">
        <div class="col-xl-3 col-sm-6 mb-xl-0">
          <div class="card overflow-hidden mb-4">
            <div class="card-header text-uppercase text-center py-3">
              <h6>Admin Panel</h6>
              <hr class="horizontal">
              <div class="row">
                <div class="col text-center">
                  <label class="text-uppercase"> all users:<%=users%></label>
                </div>
              </div>
              <hr class="horizontal">
              <div class="row">
                <div class="col">
                  <label class="text-uppercase">a users:<%=active%></label>
                </div>
                <div class="col">
                  <label class="text-uppercase">a+ users:<%=activePro%></label>
                </div>
                <div class="col">
                  <label class="text-uppercase">s users:<%=wbUsers%></label>
                </div>
              </div>
            </div>
            <div class="vertical-menu-admin card-body ">

              <!-- All users on date -->
              <div class="row gx-4">
                <div class="col mx-2 text-center">

                  <div class="" id="id_activation">
                  <%  if(typeof id_activation !== 'undefined'){ %>
                      <% id_activation.forEach(function(id){ %>

                        <div class="">
                          <form class="" action="/api/id_activation" method="post">
                            <label for=""> Email</label>
                            <p class=" text-dark text-uppercase text-sm">
                              <%= id.email %>
                            </p>
                            <label for="">Transaction ID</label>
                            <p class=" text-dark text-uppercase text-sm">
                              <%=id.transaction_id %>
                            </p>
                            <input type="text" hidden name="email" value="<%=id.email%>">
                            <input type="text" hidden name="transaction_id" value="<%=id.transaction_id %>">
                            <input type="text" hidden name="status" value="<%=id.status%>">
                            <input type="text" hidden name="activate" value="success">
                            <div class="col">
                              <button type="submit" class="btn btn-block badge text-uppercase text-center btn-success" name="button"><%=id.status%></button>
                            </div>
                          </form>
                          <form class="" action="/api/id_activation" method="POST">
                            <input type="text" hidden name="email" value="<%=id.email%>">
                            <input type="text" hidden name="transaction_id" value="<%=id.transaction_id %>">
                            <input type="text" hidden name="status" value="<%=id.status%>">
                            <input type="text" hidden name="activate" value="fail">
                            <div class="col">
                              <button type="submit" class="btn btn-block badge text-uppercase text-center btn-danger" name="button">Cancel</button>
                            </div>
                          </form>
                          <hr class="horizontal dark">
                        </div>
                      <% }) %>
                  <%  } %>

                  </div>
                   <div class="card">
                     <div class="card-body">
                       <form class="" action="/users" method="post">
                         <label class="text-uppercase text-center">All users on date</label>
                         <div class="row">
                           <div class="px-0 ">
                             <input type="text" required name="date" class="form-control text-center" placeholder="Enter Date">
                           </div>
                           <div class="px-0 ">
                             <input type="text" required name="users" hidden class="form-control text-center" value="all">
                           </div>

                         </div>
                         <br>
                         <div class="">
                           <button type="submit" class="btn btn-primary text-uppercase btn-block pt-2" name="button">Search</button>
                         </div>
                       </form>
                     </div>
                   </div>
                </div>
              </div>
              <!-- Active users on date -->
              <div class="row gx-4">
                <div class="col mx-2 text-center">
                   <div class="card">
                     <div class="card-body">
                       <form class="" action="/users" method="post">
                         <label class="text-uppercase text-center">Active users</label>
                         <div class="row">
                           <div class="px-0 ">
                             <input type="text" required name="date" class="form-control text-center" placeholder="Enter Date">
                           </div>
                           <div class="px-0 ">
                             <input type="text" required name="users" hidden class="form-control text-center" value="active">
                           </div>

                         </div>
                         <br>
                         <div class="">
                           <button type="submit" class="btn btn-primary text-uppercase btn-block pt-2" name="button">Search</button>
                         </div>
                       </form>
                     </div>
                   </div>
                </div>
              </div>
              <!-- User based on email -->
              <div class="row gx-4">
                <div class="col mx-2 text-center">
                   <div class="card">
                     <div class="card-body">
                       <form class="" action="/user-detail" method="post">
                         <label class="text-uppercase text-center">User details</label>
                         <div class="row">
                           <div class="px-0 ">
                             <input type="text" oninput="this.value = this.value.toLowerCase()" required name="email" class="form-control text-center" placeholder="Enter email">
                           </div>

                         </div>
                         <br>
                         <div class="">
                           <button type="submit" class="btn btn-primary text-uppercase btn-block pt-2" name="button">Search</button>
                         </div>
                       </form>
                     </div>
                   </div>
                </div>
              </div>
              <!-- User based on sponsor id -->
              <div class="row gx-4">
                <div class="col mx-2 text-center">
                   <div class="card mb-0">
                     <div class="card-body">
                       <form class="" action="/user-detail" method="post">
                         <label class="text-uppercase text-center">User details</label>
                         <div class="row">
                           <div class="px-0 ">
                             <input type="text" required name="sponID" class="form-control text-center" placeholder="Enter sponsor id">
                           </div>

                         </div>
                         <br>
                         <div class="">
                           <button type="submit" class="btn btn-primary text-uppercase btn-block pt-2" name="button">Search</button>
                         </div>
                       </form>
                     </div>
                   </div>
                </div>
              </div>
              <!-- Full report -->
              <div class="row gx-4">
                <div class="col mx-2 text-center">
                   <div class="card">
                     <div class="card-body">
                       <form class="" action="/report" method="post">
                         <label class="text-uppercase text-center">Get report</label>
                         <div class="row">
                           <div class="px-0 ">
                             <input type="text" required name="date" class="form-control text-center" placeholder="Enter Date">
                           </div>
                         </div>
                         <br>
                         <div class="">
                           <button type="submit" class="btn btn-primary text-uppercase btn-block pt-2" name="button">Confrim</button>
                         </div>
                       </form>
                     </div>
                   </div>
                </div>
              </div>
            </div>
            </div>
          </div>
        </div>
      </div>

  <%   if(typeof withdrawals != 'undefined'){ %>
        <div class="container-fluid  py-4">
          <div class="row">
            <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
              <div class="card overflow-hidden mb-4">
                <div class="card-header text-uppercase text-center py-3">
                  <h6>WITHDRAW</h6>
                </div>
                <div class="vertical-menu-2 card-body ">
                  <!-- New withdrawal section -->
                  <div class="row gx-4">
                    <div class="col text-center">
                    <%  withdrawals.forEach(function(request){ %>
                       <div class="card">
                         <div class="card-body ">
                           <form class="" action="/credit" method="POST">
                             <label for=""> name</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.name %>
                             </p>
                             <label for="">account number</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.accountNumber %>
                             </p>
                             <label for=""> email</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.email %>
                             </p>
                             <div class="">
                               <input type="text" name="email" hidden class="form-control text-center" value="<%=request.email %>">
                               <input type="text" name="success" hidden class="form-control text-center" value="yes">
                               <input type="text" name="payment_id" hidden class="form-control text-center" value="<%=request.payment_id %>">
                               <input type="text" name="date" hidden class="form-control text-center" value="<%=request.date %>">
                               <input type="text" name="amount" hidden class="form-control text-center" value="<%=request.amount %>">
                               <input type="text" name="from" hidden class="form-control text-center" value="<%=request.from %>">
                             </div>

                             <label for=""> ifsc</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.ifsc %>
                             </p>
                             <label for=""> amount</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.amount %>$
                             </p>
                             <label for=""> bankName</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.bankName %>
                             </p>
                             <label for=""> date</label>
                             <p class=" text-dark text-uppercase text-sm">
                               <%=request.date %>
                             </p>
                             <div class="col">
                               <button type="submit" class="btn btn-block badge text-uppercase text-center btn-success" name="button">success</button>
                             </div>
                           </form>
                             <div class="row">
                               <form class="" action="/credit" method="POST">
                                 <div class="">
                                   <input type="text" name="email" hidden class="form-control text-center" value="<%=request.email %>">
                                   <input type="text" name="success" hidden class="form-control text-center" value="no">
                                   <input type="text" name="payment_id" hidden class="form-control text-center" value="<%=request.payment_id %>">
                                   <input type="text" name="date" hidden class="form-control text-center" value="<%=request.date %>">
                                   <input type="text" name="amount" hidden class="form-control text-center" value="<%=request.amount %>">
                                   <input type="text" name="from" hidden class="form-control text-center" value="<%=request.from %>">
                                 </div>

                                 <button type="submit" class="btn btn-block badge text-uppercase text-center btn-danger" name="button">Fail</button>
                               </form>
                             </div>
                         </div>
                       </div>
                      <% }) %>
                    </div>
                  </div>
                </div>
                </div>
              </div>
            </div>
          </div>
      <% } %>




  </main>
  <!--   Core JS Files   -->

  <script src="assets/vendor/jquery/jquery.min.js"></script>
  <script src="../assets/js/core/popper.min.js"></script>
  <script src="../assets/js/core/bootstrap.min.js"></script>
  <script src="../assets/js/plugins/perfect-scrollbar.min.js"></script>
  <script src="../assets/js/plugins/smooth-scrollbar.min.js"></script>
  <script src="../assets/js/plugins/chartjs.min.js"></script>
  <script>
    var win = navigator.platform.indexOf('Win') > -1;
    if (win && document.querySelector('#sidenav-scrollbar')) {
      var options = {
        damping: '0.5'
      }
      Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
    }
  </script>
  <!-- Github buttons -->
  <script async defer src="https://buttons.github.io/buttons.js"></script>
  <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
  <script src="../assets/js/argon-dashboard.js?v=2.0.2"></script>
</body>

</html>
