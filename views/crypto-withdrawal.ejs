                          <label class="mb-1 text-dark text-uppercase text-sm text-center">
                            LTC <img src="assets/img/litecoin.png" class='coin-icon' alt="">
                          </label>
                          <div class="form-check pl-1 form-switch">
                            <input onclick="ltc_coin()" class="form-check-input" name="litecoin" type="checkbox" id="ltc_coin">
                          </div>
                        </div>
                      </div>
                      <hr class="horizontal dark">
                      <div class="text-center">
                        <label for="">Only enter the selected coin address </label>
                        <div class="col px-0 ">

                          <input type="text" required name="address" id="address" autocomplete="off" class="form-control text-center" placeholder="Enter crypto wallet address">
                        </div>
                      </div>
                      <br>
                      <div class="text-center">
                        <div class="">
                          <button type="button" onclick="validate()" class="btn btn-success btn-block pt-2" name="button">Withdraw</button>
                        </div>
                          </div>
                  </div>

                </div>
              </div>
            </div>
