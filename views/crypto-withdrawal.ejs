    <div class="container-fluid  pt-6 py-4">
      <div class="row">
        <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
          <div class="card" id="card_body">

            <div class=" py-3 ">
              <p class="text-sm text-center mb-0 text-uppercase font-weight-bold">
                Withdraw
              </p>
            </div>
            <hr class="horizontal dark">
              <div class="card-body p-3" >
               <div class="" id="alert_div">

               </div>
                <div class="row gx-4">
                  <div class="">

                    <div class="col mx-5 text-center">

                      <label for="">Confirm withdraw</label>
                      <div class="row">
                        <div class="col px-0">
                          <select name="type" class="custom-select form-control  custom-input-right" id="income_type">
                            <option value="referral" selected>Referral</option>
                            <option value="autobot">Auotobot</option>
                            <option value="superUser">Level</option>
