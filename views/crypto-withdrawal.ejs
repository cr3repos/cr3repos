                              <option value="trial">Trial</option>
                              <option value="arc">Arc</option>
                              <%   } %>
                            </select>
                          </div>
                          <div class="col-6 px-0 ">
                            <input type="number" required name="amount" id="amount" autocomplete="off" class="form-control custom-input-left" placeholder="Enter amount">
